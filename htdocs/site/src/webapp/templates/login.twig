{% extends 'base.twig' %}



{% block content %}
{% autoescape %}
<h2>Login</h2>

{{ flash.error }} <br />

<form action="" method="POST">
    <label>Username: </label>
    <input type="text" name="username" value="{{ cookieUsername }}"> <br />
        {#<input type="text" name="username" value="{{Handler::get_cookie_username()}}"> <br />#}
    <label>Password: </label>
    <input type="password" name="password"> <br />
    <input type="submit" name="submit" value="Log in">
</form>
{% endautoescape %}
{% endblock %}

